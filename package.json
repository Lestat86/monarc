{
  "name": "monarc",
  "version": "3.1.0",
  "description": "MONARC's Obviously Not A Redux Clone",
  "author": "Matteo Cavalleri",
  "homepage": "https://cvlmtg.github.io/monarc/",
  "repository": {
    "url": "git+https://github.com/cvlmtg/monarc.git",
    "type": "git"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "module": "dist/monarc.esm.js",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "analyze": "size-limit --why",
    "build": "dts build",
    "clean": "rm -rf dist",
    "lint": "eslint --ext .tsx,.jsx,.ts,.js src/ test/",
    "prepare": "dts build",
    "size": "size-limit",
    "start": "dts watch",
    "test": "dts test --passWithNoTests"
  },
  "keywords": [
    "react",
    "context",
    "store",
    "hooks",
    "autosave",
    "undo",
    "redo",
    "reducer",
    "state",
    "functional",
    "immutable",
    "flux",
    "redux"
  ],
  "peerDependencies": {
    "react": ">=17.0"
  },
  "devDependencies": {
    "@testing-library/react": "^12.0.0",
    "@types/jest": "^27.0.1",
    "@types/jsan": "^3.1.2",
    "@size-limit/preset-small-lib": "^5.0.3",
    "@types/react": "^17.0.20",
    "@types/react-dom": "^17.0.9",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "dts-cli": "^0.17.0",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-react": "^7.25.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^7.0.2",
    "immutable": "^4.0.0-rc.14",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "redux-devtools-extension": "^2.13.9",
    "size-limit": "^5.0.3",
    "tslib": "^2.3.1",
    "typescript": "^4.4.2"
  },
  "dependencies": {
    "@redux-devtools/serialize": "^0.3.0",
    "jsan": "^3.1.13",
    "tiny-invariant": "^1.1.0"
  },
 "size-limit": [
    {
      "path": "dist/monarc.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/monarc.esm.js",
      "limit": "10 KB"
    }
  ]
